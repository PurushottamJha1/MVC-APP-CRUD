


<div class="modal" id="editItemModal" tabindex="-1" role="dialog">
   
    <form id="editItemForm" asp-controller="Home" asp-action="Edit" method="post">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Edit Item</h5>
                   
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label for="editItemName">Profile Name:</label>
                        <input type="hidden" class="form-control" id="editItemId" name="editItemId" value="editItemId" > <!-- Hidden field to store the ID -->
                        <input type="text" class="form-control" id="editItemName" name="editItemName" value=listtt.ProfileName>
                    </div>
                    <div class="form-group">
                        <label for="editItemValue">Profile Url:</label>
                        <input type="text" class="form-control" id="editItemValue" name="editItemValue" value=listtt.ProfileUrl>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary">Save changes</button>
                   
                </div>
            </div>
        </div>
    </form>
</div>

